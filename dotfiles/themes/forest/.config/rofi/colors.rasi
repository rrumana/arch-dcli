* {
    /* Primary accent color (using cyan/green as primary) */
    primary: #9ed8dd;
    primary-container: #62e2a4;
    on-primary: #020802;
    on-primary-container: #fdfffd;
    
    /* Secondary accent */
    secondary: #e0eb7a;
    secondary-container: #518a51;
    on-secondary: #020802;
    on-secondary-container: #fdfffd;
    
    /* Surface backgrounds (progressively lighter) */
    surface: #020802;
    surface-dim: #020802;
    surface-bright: #518a51;
    surface-container-lowest: #020802;
    surface-container-low: #020802;
    surface-container: #518a51;
    surface-container-high: #518a51;
    surface-container-highest: #518a51;
    
    /* Text colors */
    on-surface: #fdfffd;
    on-surface-variant: #bff2ab;
    
    /* Outline/border colors */
    outline: #518a51;
    outline-variant: #518a51;
    
    /* Error colors */
    error: #bf5a7c;
    on-error: #020802;
    
    /* Inverse colors */
    inverse-surface: #fdfffd;
    inverse-on-surface: #020802;
}
